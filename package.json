{
  "name": "itsquiz-wall",
  "version": "0.0.1",
  "description": "QuizWall for itsquiz.com",
  "author": {
    "name": "WebbyLab",
    "email": "office@webbylab.com",
    "url": "http://webbylab.com"
  },
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV='production' webpack -p",
    "start": "cross-env NODE_ENV='production' BABEL_ENV='node' node server/runner.js",
    "docker:start": "docker run -dp 3001:3001 --name=itsquiz-wall registry.gitlab.webbylab.com/itsquiz/itsquiz/apps/itsquiz-wall",
    "docker:stop": "docker stop itsquiz-wall && docker rm itsquiz-wall",
    "nodemon": "cross-env BABEL_ENV='node' nodemon server/runner.js",
    "webpack-devserver": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8050 --host 0.0.0.0",
    "extract_lang": "./bin/extract_lang.sh",
    "lint": "eslint shared client server --ext js,jsx",
    "lint:fix": "eslint shared client server --ext js,jsx --fix",
    "test": "npm run lint",
    "po2json": "./bin/po2json.sh"
  },
  "engines": {
    "node": ">=6.0.0"
  },
